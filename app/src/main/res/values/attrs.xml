<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExtraRingtonePreference">
        <attr name="ringtoneType">
            <!-- Ringtones. -->
            <flag name="ringtone" value="1" />
            <!-- Notification sounds. -->
            <flag name="notification" value="2" />
            <!-- Alarm sounds. -->
            <flag name="alarm" value="4" />
            <!-- All available ringtone sounds. -->
            <flag name="all" value="7" />
        </attr>
        <attr name="showSilent" format="boolean" />
        <attr name="showDefault" format="boolean" />
        <attr name="extraRingtones" format="reference" />
        <attr name="extraRingtoneTitles" format="reference" />
    </declare-styleable>

    <declare-styleable name="PullBackView">
        <attr name="pullbackPercentOfViewMax" format="integer" />
    </declare-styleable>

    <declare-styleable name="TimerPreference">
        <attr name="maxMinutes" format="integer" />
        <attr name="minMinutes" format="integer" />
        <attr name="maxSeconds" format="integer" />
        <attr name="minSeconds" format="integer" />
        <attr name="maxExternalMinutes" format="string" />
        <attr name="minExternalMinutes" format="string" />
        <attr name="maxExternalSeconds" format="string" />
        <attr name="minExternalSeconds" format="string" />
        <attr name="secondsTitle" format="string" />
        <attr name="minutesTitle" format="string" />
        <attr name="defaultValueSeconds" format="integer" />
        <attr name="defaultValueMinutes" format="integer" />
    </declare-styleable>

    <declare-styleable name="PatternEntryView">
        <attr name="markedColor" format="color" />
        <attr name="drawColor" format="color" />
        <attr name="animTime" format="integer" />
        <attr name="drawWidth" format="float"/>
    </declare-styleable>

    <declare-styleable name="PasscodeEntryDisplay">
        <attr name="instruction" format="string" />
        <attr name="wrongPasscode" format="string" />
        <attr name="lockout1Text" format="string" />
        <attr name="lockout2Text" format="string"/>
        <attr name="lockout3Text" format="string" />
        <attr name="displayTime" format="integer" />
        <attr name="lockout1Delay" format="integer" />
        <attr name="lockout2Delay" format="integer" />
        <attr name="lockout3Delay" format="integer" />
        <attr name="lockoutLevelThreshold" format="integer" />
    </declare-styleable>
</resources>